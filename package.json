{
  "name": "pompom-time",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "Nath Paiva",
  "scripts": {
    "hasura": "hasura console --project hasura-pompom",
    "docker:stop": "docker compose -f docker-compose.yml stop",
    "docker": "docker compose -f docker-compose.yml up -d",
    "dev": "yarn docker && ntl dev",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:serverless": "vitest --config serverless/vite.config.ts --run",
    "test:serverless:watch": "vitest --config serverless/vite.config.ts",
    "test:serverless:ui": "yarn test:serverless:watch --coverage --ui",
    "test:serverless:coverage": "vitest --config serverless/vite.config.ts --coverage --run",
    "test:serverless:ci": "yarn test:serverless:coverage",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "yarn test:watch --coverage --ui",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "yarn test:coverage",
    "lint:client": "tsc --project tsconfig.json --noEmit && eslint client --ext .ts,.tsx",
    "lint:serverless": "tsc --project tsconfig.json --noEmit && eslint serverless --ext .ts,.tsx",
    "lint": "yarn lint:client && yarn lint:serverless",
    "format:client": "eslint client --ext .js,.jsx,.ts,.tsx --fix",
    "format:serverless": "eslint serverless --ext .js,.jsx,.ts,.tsx --fix",
    "format": "yarn format:client && yarn format:serverless",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@netlify/functions": "^2.0.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "netlify-identity-widget": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-netlify-identity": "^0.2.7",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@nathpaiva/eslint-config-react": "1.0.14",
    "@nathpaiva/prettier-config": "^1.0.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/netlify-identity-widget": "^1.9.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "framer-motion": "^10.16.4",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  }
}
