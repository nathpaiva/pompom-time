{
  "name": "pompom-time",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "author": "Nath Paiva",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ui": "yarn test:watch --coverage --ui",
    "test:ci": "yarn test:coverage",
    "coverage": "vitest run --coverage",
    "lint": "tsc --project tsconfig.json --noEmit && eslint src --ext .ts,.tsx",
    "format": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@netlify/functions": "^2.0.2",
    "framer-motion": "^10.16.4",
    "netlify-identity-widget": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-netlify-identity": "^0.2.7",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@nathpaiva/eslint-config-react": "1.0.14",
    "@nathpaiva/prettier-config": "^1.0.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/netlify-identity-widget": "^1.9.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
